{
  "name": "Listen",
  "author": "Erdem Dur",
  "version": "0.0.1",
  "description": "Listen to your bookmarks",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Listen",
      "default_icon": "listen-icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
